{
  "name": "@sly-cli/sly",
  "version": "1.0.0",
  "description": "Sly is a CLI tool to add components, icons, and utilities as code, not dependencies.",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "Jacob Paris",
    "url": "https://twitter.com/jacobmparis"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobparis-insiders/sly.git",
    "directory": "cli"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "jacobparis"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "start:dev": "REGISTRY_URL=http://localhost:3000/registry node dist/index.js",
    "start": "node dist/index.js",
    "release": "changeset version",
    "pub": "npm build && npm publish --access public",
    "test": "vitest run"
  },
  "dependencies": {
    "cachified": "^3.5.4",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "cosmiconfig": "^8.2.0",
    "execa": "^7.1.1",
    "lru-cache": "^10.0.0",
    "ora": "^6.3.1",
    "prompts": "^2.4.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.4",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  }
}
